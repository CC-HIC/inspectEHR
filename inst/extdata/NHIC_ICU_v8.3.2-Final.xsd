            <xs:element name="site" type="xs:string"/>
            <!--Theme name. Expected to be 'ICU'-->
            <xs:element name="theme" type="xs:string"/>
            <!--Dataset version. Expected to be 'v8.3.2'-->
            <xs:element name="datasetVersion" type="xs:string"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="data">
      <xs:complexType>
         <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="d:subject"/>
         </xs:sequence>
